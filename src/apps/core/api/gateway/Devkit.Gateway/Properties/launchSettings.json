        "iisExpress": {
            "applicationUrl": "http://localhost:8801",
            "sslPort": 44301
        }
    },
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "profiles": {
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": true,
