                "OCELOT_CONFIG_PATH": "../../../infrastructure/configs/gateway/mobile",
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "WebApplication1": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "home",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "applicationUrl": "https://localhost:44301;http://localhost:8801"
